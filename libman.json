{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.0",
      "files": [
        "jquery.min.js"
      ],
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.6.0",
      "files": [
        "dist/js/bootstrap.bundle.min.js",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap-grid.min.css",
        "dist/css/bootstrap-reboot.min.css",
        "dist/js/bootstrap.bundle.min.js.map",
        "dist/css/bootstrap.min.css.map",
        "LICENSE"
      ],
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jquery-validate@1.19.2",
      "files": [
        "jquery.validate.min.js",
        "additional-methods.min.js"
      ],
      "destination": "wwwroot/lib/jquery-validation/dist/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ],
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "provider": "jsdelivr",
      "library": "bs-custom-file-input@1.3.4",
      "files": [
        "dist/bs-custom-file-input.min.js",
        "dist/bs-custom-file-input.min.js.map",
        "README.md"
      ],
      "destination": "wwwroot/lib/bs-custom-file-input"
    }
  ]
}